#include <stdio.h>

int sum(int a, int b){
    return a+b;
}
int main() 
{
    int a, b, c;
    a = 9;
    b = 89;
    c = sum(a, b);
    printf("%d", c);
    return 0;
}
------------------------------------

#include <stdio.h>
void printstr(int n)
{
    for (int i = 0; i < n; i++)
    {
        printf("%c", '*');
    }
    
}
int main() 
{
    
    printstr(6);
    return 0;
}

